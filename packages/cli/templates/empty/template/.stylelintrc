{
  "extends": ["stylelint-config-prettier"],
  "rules": {
    "selector-type-no-unknown": null,
    "no-descending-specificity": null,
    "color-hex-case": "upper",
    "color-hex-length": "long",
    "indentation": 2,
    "max-line-length": 100,
    "unit-no-unknown": null,
    "property-no-unknown": null
  }
}
